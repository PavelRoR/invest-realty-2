#cliches
	padding 60px 0
.general-title-cliches
	padding 0 210px
	background-image: url(../img/icons/title-img-1.png), url(../img/icons/title-img-2.png)
	background-repeat: no-repeat, no-repeat
	background-position: left center, right center
.cliche
	&-wrapper
		border-radius: 8px
		padding 8px
		background-color: #232323
		background-image: linear-gradient( -48deg, #232323 0%, #0c0c0c 100%);
		box-shadow: 0px 1px 53.1px 5.9px rgba(0, 0, 0, 0.68);
		width 100%
		max-width: 286px;
		margin 0 auto
	border-radius: 8px
	background-color: transparent
	border 2px dashed rgba(#d2c2a0, .3)
	padding 23px 5px
	position relative
	text-align center
	min-height 381px
	&-title
		position absolute
		font-family: Ab
		font-size: 30px
		text-transform uppercase
		color #d2c2a0
		opacity .3
		top 0
		left 10px
	&-img
		display block
		margin 0 auto 20px
	&-text
		font-size: 16px
		span
			display block
			margin-bottom: 35px
			color #d2c2a0